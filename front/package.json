{
  "name": "portefolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css:watch": "node-sass styles/sass/main.module.scss styles/globals.css -w",
    "dev": "concurrently -k -n \"NextJS, CSS\" -p \"[{name}]\" -c \"blue, green\"  \"next dev\" \"npm run css:watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "next": "12.0.10",
    "node-sass": "^6.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.49.7"
  },
  "devDependencies": {
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10"
  }
}
